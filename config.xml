<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.contacts" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<preference name="phonegap-version" value="cli-9.0.0" />
  <name>Contacts</name>
  <description>
      Esta es una App de PhoneGap contactos contact0s
  </description>
  <author email="Luis Eduardo Garcia Caltzoncin" href="http://cordova.io">
      Apache Cordova Team
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
  <plugin name="cordova-plugin-dialogs"/> 
  <plugin name="cordova-plugin-device"/>
   <plugin name="cordova-plugin-contacts"/>
   
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <gap:platforms>
<gap:platform name="android" minVersion="2.1" />
<gap:platform name="webos" />
<gap:platform name="symbian.wrt" />
<gap:platform name="blackberry" project="widgets"/>
</gap:platforms>
<feature name="Contacts">
    <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
</feature>
<feature name="http://api.phonegap.com/1.0/contacts" />
<feature id="blackberry.pim.Contact" />
</widget>



